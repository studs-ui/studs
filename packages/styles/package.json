{
  "name": "@studs/styles",
  "version": "0.1.3",
  "type": "module",
  "main": "./index.ts",
  "exports": {
    "./components/*.scss": "./lib/components/*.scss",
    "./components/*.scss?inline": "./lib/components/*.scss?inline",
    "./studs-base.css": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    },
    ".": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "sass": "^1.64.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.3"
  }
}
